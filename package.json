{
  "name": "yunaforseyfert",
  "version": "1.0.0",
  "description": "Yunathings for seyfert",
  "main": "./build/index.js",
  "type": "commonjs",
  "types": "./types/package/index.d.ts",
  "scripts": {
    "idx": "tsc && cd src/bot-test/ && node --env-file=.env ../../dist/bot-test/index.js",
    "yuna": "mkdir -p .npm && npm run build && node dist/package/lib/prepublish.js && cp -r build .npm/build && cp -r types .npm/types",
    "prepare": "npm run build && npx husky",
    "build:esbuild": "npx esbuild --bundle src/package/index.ts --minify --outdir=build --platform=node --packages=external",
    "build": "tsc && npm run build:esbuild",
    "tsc": "tsc",
    "lint": "biome lint --write ./src",
    "format": "biome check --write ./src",
    "test": "vitest"
  },
  "keywords": [
    "parser",
    "penguin"
  ],
  "author": "SagiriIkeda",
  "license": "ISC",
  "dependencies": {
    "discord-api-types": "^0.37.90",
    "seyfert": "github:tiramisulabs/seyfert"
  },
  "homepage": "https://github.com/SagiriIkeda/yunaforseyfert#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SagiriIkeda/yunaforseyfert.git"
  },
  "lint-staged": {
    "*.ts": [
      "biome check --write",
      "biome format --write"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@types/node": "^20.14.2",
    "esbuild": "^0.20.2",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "typescript": "^5.0.0",
    "vitest": "^1.5.2"
  }
}
